{
  "name": "rohid-dev",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "sass:dev": "sass src/styles/main.scss:src/styles/main.css --watch",
    "sass:build": "sass src/styles/main.scss:src/styles/main.css",
    "eleventy:dev": "eleventy --serve",
    "eleventy:build": "eleventy",
    "dev": "concurrently \"yarn sass:dev\" \"yarn eleventy:dev\"",
    "build": "yarn sass:build && yarn eleventy:build"
  },
  "devDependencies": {
    "@11ty/eleventy": "11ty/eleventy",
    "concurrently": "^7.3.0",
    "sass": "^1.54.8"
  }
}
